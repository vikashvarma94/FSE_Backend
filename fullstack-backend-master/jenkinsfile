pipeline {
    agent any
    stages {
        stage('Commit SCM') {
//             agent{
//                 label docker{
//                     image 'maven'
//                     args '-v $HOME/.m2:/root/.m2'
//                 }
//             }
            steps {
                script{
                    echo 'MVN Build'
                    sh "mvn clean install"
                }
            }
        }
        stage('Build') {
            steps{
                script{
                    sh 'docker build . -t vikashvarma94/fseb:1'
                    withCredentials([string(credentialsId: '', variable: 'docker_password')])
                    sh 'docker login -u vikashvarma94 -p $docker_password'
                    sh 'docker push vikashvarma94/fseb:1'
                }
            }

        }
    }
    }